{
  "name": "td",
  "version": "0.0.1",
  "bin": {
    "td": "./dist/src/cli/index.js"
  },
  "scripts": {
    "predebug": "npm run build",
    "debug": "node ./dist/src/cli/index.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --forceExit --detectOpenHandles",
    "coverage": "npm run test -- --coverage",
    "clean": "rm -fr dist",
    "prebuild": "npm run clean",
    "build": "tsc",
    "deploy": "npm run build && npm uninstall -g td && npm install -g . -f"
  },
  "dependencies": {
    "chalk": "~4",
    "commander": "^9.4.1",
    "fs-extra": "^10.1.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "shelljs": "^0.8.5",
    "shelljs.exec": "^1.1.8"
  },
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^29.2.2",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.187",
    "@types/node": "^18.11.7",
    "@types/shelljs": "^0.8.11",
    "@types/shelljs.exec": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "eslint": ">=5.16.0",
    "jest": "^29.3.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
